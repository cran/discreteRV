\name{P}
\alias{P}
\title{Calculate probabilities of events}
\usage{
  P(event)
}
\arguments{
  \item{event}{A logical vector, with names equal to the
  probabilities}
}
\description{
  Calculate probabilities of events
}
\examples{
X.fair.die <- make.RV(1:6, rep("1/6",6))
P(X.fair.die>3)

X.loaded.die <- make.RV(1:6, c(1,1,1,1,2,4))
P(X.loaded.die>3)
P(X.loaded.die==6)
}

